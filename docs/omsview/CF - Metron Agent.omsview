                                    },
                                    "Header": {
                                        "Title": "Bytes Received by Agent Listener per Minute",
                                        "Subtitle": "by Environment"
                                    },
                                    "LineChart": {
                                        "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent Name_s=\"dropsondeAgentListener.receivedByteCount\" | measure sum(div(Delta_d, 5240)) by Environment_s interval 5Minutes",
                                        "yAxis": {
                                            "isLogarithmic": false,
