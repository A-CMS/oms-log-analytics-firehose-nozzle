                        "OverviewTile": {
                            "Id": "LineChartBuilderTile",
                            "Type": "OverviewTile",
                            "Version": 0,
                            "Configuration": {
                                "LineChart": {
                                    "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent Name_s:contains(\"sentMessageCount\") | measure sum(div(Delta_d,5)) by Environment_s interval 5Minutes",
                                    "yAxis": {
                                        "isLogarithmic": false,
                                        "units": {
                                            "baseUnitType": "Count",
                                            "baseUnit": "Ones",
                                            "displayUnit": "Ones"
                                        },
