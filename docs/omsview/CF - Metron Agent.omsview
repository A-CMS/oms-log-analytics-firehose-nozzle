                                        {
                                            "Header": {
                                                "Title": "Messages Sent to Doppler per Minute",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent Name_s:contains(\"sentMessageCount\") | measure sum(div(Delta_d,5)) by Environment_s,Name_s interval 5Minutes",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": ""
