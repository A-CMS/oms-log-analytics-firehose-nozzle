                                            "LineChart": {
                                                "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent Name_s:contains(\"sentByteCount\") | measure sum(div(Delta_d,5242880)) by Environment_s,Name_s interval 5Minutes",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Bits",
                                                        "baseUnit": "Megabytes",
                                                        "displayUnit": "Megabytes"
                                                    },
                                                    "customLabel": ""
