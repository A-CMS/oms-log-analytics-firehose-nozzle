                                    "LineChart": {
                                        "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent (Name_s:contains(\"dropsondeUnmarshaller\") and Name_s:contains(\"Received\")) or Name_s=\"dropsondeUnmarshaller.logMessageTotal\" | measure sum(div(Delta_d,5)) by Environment_s,Name_s interval 5Minutes",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
