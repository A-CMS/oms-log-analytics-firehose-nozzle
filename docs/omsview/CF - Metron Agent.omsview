                                            "LineChart": {
                                                "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent Name_s:contains(\"sendErrorCount\") | measure max(Total_d) by Environment_s,Name_s interval 5Minutes",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
