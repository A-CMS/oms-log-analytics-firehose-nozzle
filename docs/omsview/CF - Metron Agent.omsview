                                    "LineChart": {
                                        "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent Name_s=\"dropsondeUnmarshaller.receivedEnvelopes\" | measure sum(div(Delta_d,5)) by Environment_s interval 5Minute",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "Count",
                                                "baseUnit": "Ones",
                                                "displayUnit": "Ones"
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent Name_s=\"dropsondeUnmarshaller.receivedEnvelopes\" | measure max(Total_d) by Environment_s,Job_s,IP_s",
