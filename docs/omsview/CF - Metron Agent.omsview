                                        "Query": "Type=CF_CounterEvent_CL Origin_s=MetronAgent (Name_s:contains(\"dropsondeUnmarshaller\") and Name_s:contains(\"Received\")) or Name_s=\"dropsondeUnmarshaller.logMessageTotal\" | measure max(Total_d) by Environment_s,Name_s,IP_s",
                                        "HideGraph": true,
                                        "enableSparklines": false,
                                        "operation": "Last Sample",
                                        "ColumnsTitle": {
                                            "Name": "Environment",
                                            "Value": "Total"
                                        },
                                        "Color": "#0072c6",
